<h1>Listing brands</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Email</th>
    <th>Logo</th>
    <th>Phone number</th>
    <th>Website</th>
    <th>Location</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Contact name</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @brands.each do |brand| %>
  <tr>
    <td><%= brand.name %></td>
    <td><%= brand.description %></td>
    <td><%= brand.email %></td>
    <td><%= brand.logo %></td>
    <td><%= brand.phone_number %></td>
    <td><%= brand.website %></td>
    <td><%= brand.location %></td>
    <td><%= brand.latitude %></td>
    <td><%= brand.longitude %></td>
    <td><%= brand.contact_name %></td>
    <td><%= brand.user_id %></td>
    <td><%= link_to 'Show', brand %></td>
    <td><%= link_to 'Edit', edit_brand_path(brand) %></td>
    <td><%= link_to 'Destroy', brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Brand', new_brand_path %>
