<h1>Listing events</h1>

 <div class="row-fluid">
    <ul class="thumbnails">
        <% @events.each do |event| %>
            <%= render :partial => "event", :locals => { :event => event } %>
        <% end %>
    </ul>

</div>

<br />

<%= link_to 'New Event', new_event_path, :class => 'btn btn-primary' %>
